<?xml version="1.0" encoding="UTF-8"?>
<entity-mappings
    xmlns="http://java.sun.com/xml/ns/persistence/orm"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://java.sun.com/xml/ns/persistence/orm 
                        http://java.sun.com/xml/ns/persistence/orm_1_0.xsd"
    version="1.0">

    <package>org.lazydog.entry.model</package>
    <entity class="ApplicationUser" access="FIELD" metadata-complete="true">
        <table name="application_user" schema="entry"/>
        <named-query name="ApplicationUser.findAll">
            <query>select a from ApplicationUser a</query>
        </named-query>
        <attributes>
            <id name="id">
                <column name="id"/>
                <generated-value strategy="IDENTITY"/>
            </id>
            <basic name="emailAddress">
                <column name="email_address" nullable="false"/>
            </basic>
            <basic name="firstName">
                <column name="first_name" nullable="false"/>
            </basic>
            <basic name="lastName">
                <column name="last_name" nullable="false"/>
            </basic>
            <basic name="modifyTime">
                <column name="modify_time"/>
                <temporal>TIMESTAMP</temporal>
            </basic>
            <basic name="registerTime">
                <column name="register_time" nullable="false"/>
                <temporal>TIMESTAMP</temporal>
            </basic>
            <basic name="username">
                <column name="username" nullable="false" unique="true"/>
            </basic>
            <basic name="uuid">
                <column name="uuid" nullable="false" unique="true"/>
            </basic>
        </attributes>
    </entity>
</entity-mappings>
